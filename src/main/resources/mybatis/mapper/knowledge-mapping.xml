<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="com.starters.ityogurt.dao.KnowledgeDAO">


<select id="list" resultType="knowledgedto" parameterType="int">
	select * from knowledge where user_seq=#{userSeq}
</select>

<select id="totalCnt" resultType="int" parameterType="int">
	select count(*) from knowledge where user_seq=#{userSeq}
</select>
</mapper>